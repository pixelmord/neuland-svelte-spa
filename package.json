    "@types/chai": "^4.2.11",
    "@types/cypress": "^1.1.3",
    "@types/eslint": "^6.1.8",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/mapbox-gl": "^1.8.1",
    "@types/mocha": "^7.0.2",
    "@types/prettier": "^1.19.1",
    "cypress": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
